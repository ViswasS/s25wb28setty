extends layout

block content
  h1 Viswas
  h2 Welcome to My Express App

  - var results = [ { origin: 'Interference of Light', tone: 'Yellow', clarity: 8 },
                    { origin: 'Recording the Pattern', tone: 'Black', clarity: 6 },
                    { origin: 'Reconstructing the Image', tone: 'White', clarity: 9 }]

  .container-fluid
    .row
      each item in results
        .col-sm-2
          h5 Holograms
          p Origin: #{item.origin}
          p Tone: #{item.tone}
          p Clarity: #{item.clarity}
